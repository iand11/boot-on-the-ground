<h1>Add New Company Form</h1>


<%= form_for @company, url: {action: "create"} do |f| %>
	<%= f.label "Company Name:" %>
  <%= f.text_field :name %><br>
	<%= f.label "Company Description:" %>
  <%= f.text_area :description, size: "50x5" %> <br>
  <%= f.label "Company Logo:" %> 
  <%= f.file_field :avatar %> <br>
  <%= f.label "Company Link:" %>
  <%= f.text_field :url %><br>
  <%= f.submit "Create" %>
<% end %>